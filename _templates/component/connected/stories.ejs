---
to: src/components/<%= folder %>/<%= h.inflection.camelize(name) %>/<%= h.inflection.camelize(name) %>.stories.js
---
/* eslint no-magic-numbers: 0 */
/* eslint max-len: 0 */
import React from 'react'
import { storiesOf } from '@storybook/react'
import * as knobs from '@storybook/addon-knobs'
// import { action } from '@storybook/addon-actions'
// import _ from 'lodash'
// import faker from 'faker'
// import dayjs from 'dayjs'

import { <%= h.inflection.camelize(name) %> } from './<%= h.inflection.camelize(name) %>'

storiesOf('<%= h.inflection.titleize(folder) %>', module)
  .addDecorator(knobs.withKnobs)
  .add('<%= h.inflection.camelize(name) %>', () => {
    return (
      <<%= h.inflection.camelize(name) %>
        text={knobs.text('text', '')}
      />
    )
  })
